{
  "private": true,
  "scripts": {
    "lint": "eslint *.js src tools",
    "build": "npm run lint && npm run update-schema && npm run relay-compiler && npm run webpack",
    "relay-compiler": "relay-compiler --src src --schema src/data/schema.graphql",
    "start": "npm run relay-compiler && BABEL_ENV=es webpack-dev-server --hot --inline",
    "start-node-server": "nodemon --ignore ./assets --ignore ./data --ignore ./tools --exec \"babel-node\" src/server",
    "start-graphql-server": "nodemon --watch ./data --watch ./src/graphql-server.js --exec \"babel-node\" src/graphql-server",
    "update-schema": "babel-node tools/updateSchema.js",
    "webpack": "NODE_ENV=production BABEL_ENV=es webpack -p"
  },
